{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console" : "off",
        "linebreak-style" : "off",
        "indent" : "off",
        "eol-last" : "off",
        "key-spacing" :"off",
        "no-unused-vars" :"off",
        "space-before-blocks" :"off",
        "object-curly-newline" : "off"
    }
}
